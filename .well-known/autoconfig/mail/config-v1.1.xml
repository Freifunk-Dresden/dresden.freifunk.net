<?xml version="1.0" encoding="UTF-8"?>

<clientConfig version="1.1">
  <emailProvider id="freifunk-dresden.de">
    <domain>freifunk-dresden.de</domain>
    <displayName>freifunk-dresden.de</displayName>
    <displayShortName>Freifunk Dresden</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.freifunk-dresden.de</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <authentication>password-encrypted</authentication>
      <username>%EMAILADDRESS%</username>
